{
  "compilerOptions": {
    "outDir": "./static/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "plugins": [{
      "name": "typescript-plugin-css-modules"
    }]
  },
  "include": ["./src/**/*"]
}
